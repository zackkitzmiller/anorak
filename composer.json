{
	"name": "jbrooksuk/anorak",
	"description": "PHP code reviews with Anorak. .",
	"keywords": ["code", "reviews", "anorak"],
	"license": "MIT",
	"repositories": {
		"phpcheckstyle": {
			"type": "package",
			"package": {
				"name": "phpcheckstyle/phpcheckstyle",
				"version": "0.6.2",
				"source": {
					"url": "http://phpcheckstyle.googlecode.com/svn",
					"type": "svn",
					"reference": "tags/V0.13.1"
				}
			}
		}
	},
	"require": {
		"laravel/framework": "4.2.*",
		"league/oauth2-client": "~0.3",
		"knplabs/github-api": "*",
		"webpatser/laravel-uuid": "dev-master",
		"squizlabs/php_codesniffer": "*",
		"pda/pheanstalk": "2.0.*",
		"symfony/yaml": "~2.1",
		"phpcheckstyle/phpcheckstyle": "*",
		"laravel/cashier": "~2.0"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/providers",
			"app/controllers",
			"app/policies",
			"app/models",
			"app/jobs",
			"app/database/migrations",
			"app/database/seeds",
			"app/collections"
		],
		"psr-0": {
			"GitHub": "vendor/knplabs/github-api/lib"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
